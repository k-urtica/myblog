{
  "name": "myblog",
  "private": true,
  "description": "My personal blog powered by Gatsby",
  "version": "1.0.0",
  "author": "K",
  "scripts": {
    "build": "cross-env GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint . --ext .ts,.js,.tsx,.jsx --max-warnings=0 --ignore-path .gitignore",
    "lint-fix": "yarn prettier-fix && eslint . --fix --ext --ext .ts,.js,.tsx,.jsx --max-warnings=0 --ignore-path .gitignore",
    "prettier-check": "prettier --check ./src/**/*.{js,jsx,ts,tsx,json}",
    "prettier-fix": "prettier --write ./src/**/*.{js,jsx,ts,tsx,json}"
  },
  "dependencies": {
    "@mdi/js": "^5.9.55",
    "@mdi/react": "^1.5.0",
    "cross-env": "^7.0.3",
    "daisyui": "^2.20.0",
    "dayjs": "^1.10.4",
    "gatsby": "^4.19.2",
    "gatsby-awesome-pagination": "^0.3.8",
    "gatsby-plugin-google-gtag": "^4.19.0",
    "gatsby-plugin-image": "^2.19.0",
    "gatsby-plugin-manifest": "^4.19.0",
    "gatsby-plugin-next-seo": "^1.10.0",
    "gatsby-plugin-offline": "^5.19.0",
    "gatsby-plugin-robots-txt": "^1.7.1",
    "gatsby-plugin-sass": "^5.19.0",
    "gatsby-plugin-sharp": "^4.19.0",
    "gatsby-plugin-sitemap": "5.19.0",
    "gatsby-plugin-typegen": "^3.0.0",
    "gatsby-remark-autolink-headers": "^5.19.0",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-hard-breaks": "^1.0.0",
    "gatsby-remark-images": "^6.19.0",
    "gatsby-remark-link-card": "https://github.com/k-urtica/gatsby-remark-link-card.git",
    "gatsby-remark-prismjs": "^6.19.0",
    "gatsby-remark-prismjs-title": "^1.0.0",
    "gatsby-source-filesystem": "^4.19.0",
    "gatsby-transformer-remark": "^5.19.0",
    "gatsby-transformer-sharp": "^4.19.0",
    "lodash.kebabcase": "^4.1.1",
    "prismjs": "^1.23.0",
    "react": "^18.2.0",
    "react-adsense": "^0.1.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.0.9",
    "react-share": "^4.4.0",
    "react-tsparticles": "^1.37.4",
    "sass": "^1.32.12"
  },
  "devDependencies": {
    "@fec/remark-a11y-emoji": "^3.1.0",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@types/lodash.kebabcase": "^4.1.6",
    "@types/node": "^18.6.2",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-paginate": "^7.1.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "gatsby-plugin-postcss": "^5.19.0",
    "postcss": "^8.4.14",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.1.6",
    "typescript": "^4.7.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/k-urtica/myblog"
  }
}
